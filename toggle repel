       local _p = nil
        for _, v in pairs(getgc(true)) do
           if typeof(v) == "table" then
               if rawget(v, "PlayerData") then
                   _p = v
                   break
               end
           end
        end
        if(value) then _p.Repel.steps = 1000000
        else
        _p.Repel.steps = 0
    end
    Enabled = true
